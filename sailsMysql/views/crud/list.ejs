<form method=“POST” action=“user”>
  <table>
    <tr>
      <td>Name</td>
      <td>:</td>
      <td><input type=“text” name=“name” /></td>
    </tr>
    <tr>
      <td>Mobile</td>
      <td>:</td>
      <td><input type=“text” name=“mobile” /></td>
    </tr>
    <tr>
      <td>Country</td>
      <td>:</td>
      <td><input type=“text” name=“country” /></td>
    </tr>
    <tr>
      <td colspan=“2”></td>
      <td><input type=“submit” value=“Submit” /></td>
    </tr>
  </table>
</form>
<h1>
  <table>
      <tr>
        <td>No</td>
        <td>Name</td>
        <td>Country</td>
        <td>Mobile</td>
        <td>Configuration</td>
      </tr>
      <% for(var i=0; i<users.length; i++) {%>
        <tr>
          <td><%= i+1 %></td>
          <td><%= users[i].name %></td>
          <td><%= users[i].country %></td>
          <td><%= users[i].mobile %></td>
          <td><a href=“user/<%= users[i].id %>”>Edit</a><a href=“user/delete/<%= users[i].id %>”>Delete</a></td>
      <% } %>
      </ul>
  </table>
</h1>
